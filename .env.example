# Application Configuration

# Port for the application (default: 8080)
APP_PORT=8080

# Image tag for Docker images (default: latest)
# GIGER_IMAGE_TAG=latest

# Debug Logging
# Enable verbose debug logging in backend (default: false)
# Set to "true" or "1" to enable
GIGER_DEBUG_LOGGING=false

# Frontend Debug Logging
# Enable verbose debug logging in frontend (default: false)
# Set to "true" or "1" to enable, or use localStorage.setItem('GIGER_DEBUG_LOGGING', 'true') at runtime
VITE_DEBUG_LOGGING=false
